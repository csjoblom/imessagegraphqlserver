{
  "name": "imessagegraphqlserver",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "test:report": "bun test --coverage && open coverage/lcov-report/index.html",
    "start": "bun src/index.ts",
    "typecheck": "tsc --noEmit",
    "build": "bun build ./src/index.ts --outdir ./dist --target node"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@devsnowflake/text-emoji-parser": "^1.0.0",
    "@graphql-tools/schema": "^10.0.23",
    "apollo-server": "^3.3.0",
    "blessed": "^0.1.81",
    "emoji-short-name": "^1.0.0",
    "glob": "^7.1.7",
    "graphql": "^15.5.3",
    "osa2": "^0.2.3",
    "running-at": "^0.3.22",
    "sqlite3": "^5.0.2"
  },
  "bin": "dist/index.js",
  "pkg": {
    "scripts": "dist/**/*.js",
    "assets": [
      "node_modules/sqlite3/lib/binding/napi-v3-darwin-x64/node_sqlite3.node"
    ],
    "targets": [
      "node20-mac-x64"
    ],
    "outputPath": "dist"
  },
  "private": true,
  "devDependencies": {
    "@types/blessed": "^0.1.25",
    "@types/bun": "latest",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.14",
    "@types/sqlite3": "^5.1.0",
    "bun-types": "^1.2.7",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.0"
  },
  "peerDependencies": {
    "typescript": "^5.8.2"
  }
}